{
    "ci": {
        "collect": {
            "numberOfRuns": 3,
            "settings": {
                "preset": "desktop",
                "throttlingMethod": "devtools"
            }
        },
        "upload": {
            "target": "filesystem",
            "outputDir": "./lhci-reports"
        },
        "comment-assert": {
            "preset": "lighthouse:recommended"
        }
    }
}