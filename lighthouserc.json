{
    "ci": {
        "collect": {
            "numberOfRuns": 3,
            "settings": {
                "preset": "desktop",
                "throttlingMethod": "devtools",
                "plugins": [
                    "lighthouse-plugin-field-performance"
                ]
            }
        },
        "upload": {
            "target": "filesystem",
            "outputDir": "./lhci-reports"
        },
        "comment-assert": {
            "preset": "lighthouse:recommended"
        }
    }
}